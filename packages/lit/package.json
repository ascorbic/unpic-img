{
  "name": "@unpic/lit",
  "version": "0.0.17",
  "description": "A high-performance, responsive image component for Lit",
  "main": "unpic-img.js",
  "module": "unpic-img.js",
  "exports": {
    ".": {
      "types": "./unpic-img.d.ts",
      "import": "./unpic-img.js",
      "webcomponents": "./unpic-img.js"
    }
  },
  "type": "module",
  "files": [
    "unpic-img.js",
    "unpic-img.d.ts",
    "unpic-img.js.map",
    "unpic-img.d.ts.map",
    "src/unpic-img.ts",
    "custom-elements.json"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "pnpm build && pnpm wca && pnpm publint",
    "wca": "wca analyze src --outFile custom-elements.json --format json",
    "publint": "publint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ascorbic/unpic-img",
    "directory": "packages/lit"
  },
  "homepage": "https://unpic.pics/img/lit",
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit",
    "unpic"
  ],
  "author": "Matt Kane",
  "license": "MIT",
  "peerDependencies": {
    "lit": "^2.7.2"
  },
  "devDependencies": {
    "lit": "^2.7.5",
    "publint": "^0.2.0",
    "web-component-analyzer": "2.0.0"
  },
  "dependencies": {
    "@unpic/core": "workspace:^"
  },
  "publishConfig": {
    "access": "public"
  }
}