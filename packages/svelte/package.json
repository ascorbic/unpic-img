    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.29.0"
  },
  "type": "module",
  "dependencies": {
    "@unpic/core": "workspace:^",
    "style-object-to-css-string": "^1.0.1"
  },
  "peerDependencies": {
    "svelte": "*"
  },
  "files": [
