{
  "version": "0.0.11",
  "name": "@unpic/solid",
  "description": "SolidJS component for responsive, high-performance images.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ascorbic/unpic-img",
    "directory": "packages/solid"
  },
  "homepage": "https://github.com/ascorbic/unpic-img#readme",
  "author": "ascorbic",
  "bugs": {
    "url": "https://github.com/ascorbic/unpic-img/issues"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "vite": "vite build",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ssr": "yarn run test --mode ssr",
    "prepublishOnly": "yarn clean && yarn build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "yarn up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "esbuild": "^0.17.4",
    "esbuild-plugin-solid": "^0.5.0",
    "jsdom": "^21.1.0",
    "prettier": "2.8.4",
    "solid-js": "^1.6.9",
    "solid-testing-library": "^0.5.1",
    "tsup": "^6.5.0",
    "tsup-preset-solid": "^0.1.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0",
    "vitest": "^0.29.0"
  },
  "keywords": [
    "solid"
  ],
  "dependencies": {
    "@unpic/core": "workspace:^"
  },
  "browser": {}
